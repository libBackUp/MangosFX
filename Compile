autoreconf --install --force
aclocal
autoheader
autoconf
automake --add-missing
automake src/bindings/Scriptdev2/Makefile
mkdir objdir
cd objdir
../configure --prefix=/home/nerzhul/mangos/server_test/ --sysconfdir=/home/nerzhul/mangos/server_test/etc --enable-cli --enable-ra --datadir=/home/nerzhul/mangos/server_test/dat
make
make install -j8
cd ..


